# Multiboard Sync Service - Production Environment

PORT=8080
LOG_LEVEL=info

REDIS_URL=redis://127.0.0.1:6379

PRODUCTION_DATABASE_URL=postgresql://user:password@host:5432/dbname
STAGING_DATABASE_URL=postgresql://user:password@host:5432/dbname
DEV_DATABASE_URL=postgresql://user:password@localhost:5432/dbname

MAX_DUMP_SIZE_GB=10
JOB_TIMEOUT_MINUTES=60
MAX_CONCURRENT_JOBS=2
AUTO_BACKUP=true
ENABLE_COMPRESSION=true
BACKUP_RETENTION_DAYS=7

DUMP_DIRECTORY=/var/lib/multiboard-sync-service/dumps
BACKUP_DIRECTORY=/var/lib/multiboard-sync-service/backups
